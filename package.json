{
    "name": "nextjs-demo",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "eslint ./src/ --ext .ts --ext .tsx --fix",
        "knex:migrate": "knex migrate:latest --knexfile knexfile.js",
        "knex:seed": "knex seed:run --knexfile knexfile.js",
        "knex:rollback": "knex migrate:rollback",
        "knex:dev-empty": "knex migrate:rollback && knex migrate:latest",
        "knex:dev": "knex migrate:rollback && knex migrate:latest && knex seed:run"
    },
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "next": "9.5.2",
        "react": "16.13.1",
        "react-dom": "16.13.1"
    },
    "devDependencies": {
        "@types/node": "^14.6.2",
        "@types/react": "^16.9.49",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "eslint": "^7.8.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-standard": "^4.0.1",
        "typescript": "^4.0.2"
    }
}
